Database and Table Creation

Create database Infinity;

create table Infinity.USER(User_ID int(20), User_Name varchar(50));

INSERT INTO [Infinity].[dbo].[USER](User_ID,User_Name) values (1,'User1');

INSERT INTO [Infinity].[dbo].[USER](User_ID,User_Name) values (2,'User2');

select * from [Infinity].[dbo].[USER];

------------------
User_ID User_Name
------------------
	 1	  User1
	 2	  User2
